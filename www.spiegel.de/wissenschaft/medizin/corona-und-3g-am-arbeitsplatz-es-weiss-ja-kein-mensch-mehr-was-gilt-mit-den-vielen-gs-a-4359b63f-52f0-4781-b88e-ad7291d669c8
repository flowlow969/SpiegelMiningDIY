var exportz = exportz || {};
(function(exports) {
    "use strict";
var conf = {
    'globalConfig': 'https://s3.amazonaws.com/homad-global-configs.schneevonmorgen.com/global_config.json'
    ,'clientConfig': 'https://cdn.prod.www.spiegel.de/public/spon/json/homadConfig.json'
    ,'prechecked': false
    ,'prerollAdTag': function(player) { return getPrerollAdTag(player); }
    ,'midrollAdTag': function(player) { return getAdTag(player, 'mid'); }
};

function getPrerollAdTag(player) {
    var playerConfig = player && player.getConfig() ? player.getConfig() : undefined;
    if (playerConfig && playerConfig.advertising && playerConfig.advertising.prerollAdTag) {
        return playerConfig.advertising.prerollAdTag;
    }

    return undefined;
}

function getAdTag(player, type) {
    var playerConfig = player && player.getConfig() ? player.getConfig() : undefined;
    var adTag = undefined;
    if (playerConfig
    && playerConfig.advertising
    && playerConfig.advertising.schedule
    && Array.isArray(playerConfig.advertising.schedule)
    && playerConfig.advertising.schedule.length > 0) {
        for (var i = 0; i < playerConfig.advertising.schedule.length; i++) {
            var schedule = playerConfig.advertising.schedule[i];
            if (schedule.offset
            && schedule.tag
            && Array.isArray(schedule.tag)
            && schedule.tag.length > 0) {
                if (schedule.offset !== 'pre' && schedule.offset !== 'post' && type === 'mid') {
                    if (!schedule.homadUsed) {
                        adTag = schedule.tag[0];
                        schedule.homadUsed = true;
                        break;
                    }
                } else if (schedule.offset === type) {
                    adTag = schedule.tag[0];
                    break;
                }
            }
        }
    }
    return adTag;
};
